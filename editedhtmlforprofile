<script>
//search popup

function showSearchbar() {
    document.getElementById('search-popup').style = "display: block; overflow-y: scroll;"
    document.querySelector('body').style = "overflow: hidden;"
}

function hideSearchbar() {
    document.getElementById('search-popup').style = "display: none; overflow: hidden;"
    document.querySelector('body').style = "overflow: auto;"
}
</script>
<script>
// add post pop up

function showAddpost() {
    document.getElementById('add_media_pop').style = "display: block; overflow-y: scroll;"
    document.querySelector('body').style = "overflow: hidden;"
}

function hideAddpost() {
    document.getElementById('add_media_pop').style = "display: none; overflow: hidden;"
    document.querySelector('body').style = "overflow: auto;"
}
</script>
<script>
// Create channel pop up

function showCreatechannel() {
    document.getElementById('create_channel_pop').style = "display: block; overflow-y: scroll;"
    document.querySelector('body').style = "overflow: hidden;"
}

function hideCreatechannel() {
    document.getElementById('create_channel_pop').style = "display: none; overflow: hidden;"
    document.querySelector('body').style = "overflow: auto;"
}
</script>
<script>
// Create community pop up

function showCreatecommunity() {
    document.getElementById('create_community_pop').style = "display: block; overflow-y: scroll;"
    document.querySelector('body').style = "overflow: hidden;"
}

function hideCreatecommunity() {
    document.getElementById('create_community_pop').style = "display: none; overflow: hidden;"
    document.querySelector('body').style = "overflow: auto;"
}
</script>
<script>
// Create channel form pop up

function showCreatechannelform() {
    document.getElementById('create_channel_form_pop').style = "display: block; overflow-y: scroll;"
    document.querySelector('body').style = "overflow: hidden;"
}

function hideCreatechannelform() {
    document.getElementById('create_channel_form_pop').style = "display: none; overflow: hidden;"
    document.querySelector('body').style = "overflow: auto;"
}
</script>
