<script type="text/javascript">
$('#modal-test-1').hide();

function nextClicked() {
    $('#modal-test-2').fadeOut(function() { $('#modal-test-1').fadeIn() });
};
$('#modal-test-3').hide();

function nextClicked2() {
    $('#modal-test-2').fadeOut(function() { $('#modal-test-3').fadeIn() });
};
$('#modal-test-3').hide();

function nextClicked3() {
    $('#modal-test-3').fadeOut(function() { $('#modal-test-2').fadeIn() });
}
</script>
