    document.getElementById('add_media_pop_new').style = "display: block; overflow-y: scroll;"
    document.querySelector('body').style = "overflow: hidden;"
}

function hideAddpostNew() {
    document.getElementById('add_media_pop_new').style = "display: none; overflow: hidden;"
    document.querySelector('body').style = "overflow: auto;"
}
</script>
