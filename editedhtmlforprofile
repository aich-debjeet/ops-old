<!-- for choose channel -->
<script>
jQuery(document).ready(function($) {

    $("#choose_channel_toggle_new").click(function() {

        $("#choose_channel_details_new").slideToggle("slow");

        if ($("#choose_channel_toggle_new").text() == "Choose Channel") {
            $("#choose_channel_toggle_new").html("Choose Channel")
        } else {
            $("#choose_channel_toggle_new").text("Choose Channel")
        }

    });

});
</script>
<!-- for textarea -->
<script>
function expandTextarea(id) {
    document.getElementById(id).addEventListener('keyup', function() {
        this.style.overflow = 'hidden';
        this.style.height = 0;
        this.style.height = this.scrollHeight + 'px';
    }, false);
}

expandTextarea('txtarea');
</script>